<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item>
        <rotate
            android:fromDegrees="35"
            android:toDegrees="65"
            android:pivotX="-140%"
            android:pivotY="67%" >
            <shape
                android:shape="rectangle" >
                <stroke android:color="#15455E"
                    android:width="4dp"/>
                <corners android:radius="7dp"/>
                <solid
                    android:color="#993499AA" />
            </shape>
        </rotate>
    </item>

    <item>
        <rotate
            android:fromDegrees="57"
            android:toDegrees="65"
            android:pivotX="-80%"
            android:pivotY="27%" >
            <shape
                android:shape="rectangle" >
                <corners android:radius="5dp"/>
                <solid
                    android:color="#9974AECB" />
            </shape>
        </rotate>
    </item>

    <item>
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="-60%"
            android:pivotY="87%" >
            <shape
                android:shape="rectangle" >
                <corners android:radius="10dp"/>
                <stroke android:color="#993479A4"
                    android:width="5dp"/>
                <solid
                    android:color="#3479A4" />

            </shape>
        </rotate>
    </item>
</layer-list>
